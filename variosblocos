local baseplate = game.Workspace.Baseplate -- Baseplate
local jogador = game.Players.LocalPlayer -- Jogador local

local numPartes = 50 -- Número total de partes
local cores = {BrickColor.new("Bright red"), BrickColor.new("Bright blue"), BrickColor.new("Bright green"), BrickColor.new("Bright yellow"), BrickColor.new("Bright orange")} -- Cores diferenciadas
local velocidades = {2, 4, 6, 8, 10} -- Velocidades correspondentes às cores

local function criarParte()
    local parte = Instance.new("Part")
    parte.Size = Vector3.new(5, 5, 5)
    parte.Anchored = true
    parte.CanCollide = true
    parte.Parent = baseplate
    
    return parte
end

local partes = {}
for i = 1, numPartes do
    local parte = criarParte()
    parte.Position = Vector3.new(
        math.random(baseplate.Size.X * -0.35, baseplate.Size.X * 0.35),
        5,
        math.random(baseplate.Size.Z * -0.35, baseplate.Size.Z * 0.35)
    )
    parte.BrickColor = cores[(i - 1) % #cores + 1]
    partes[i] = parte
    
    parte.Touched:Connect(function(otherPart)
        local humanoid = jogador.Character and jogador.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            local corIndex = table.find(cores, parte.BrickColor)
            if corIndex then
                humanoid.WalkSpeed = humanoid.WalkSpeed + velocidades[corIndex]
            end
        end
    end)
end
Nesse script, 50 partes são criadas e distribuídas dentro do Baseplate, com cada parte recebendo uma cor diferente da tabela cor
